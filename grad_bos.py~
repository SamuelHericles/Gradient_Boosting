x = np.arange(0, 60)
def prod(x):
  y = x / 2 + (x // 10) % 2 * 20 * x / 5 + np.random.random() * 10
  return y

y = prod(x)
x = pd.DataFrame({'x': x})

gradient_boosting_regressor = ensemble.GradientBoostingRegressor(n_estimators=10,
max_depth=5,learning_rate=1,criterion='mse')
gradient_boosting_regressor.fit(x, y)
